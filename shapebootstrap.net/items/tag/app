<script>
    jQuery('form[id="sendy-subscribe-95"]').submit(function (form)
    {
        var formId = '#' + form.target.id;
        var email = jQuery(formId + ' input[name=email]').val();
        var url = 'email=' + email + '&f58c1dec3597e7b9f2759362373e5369=1&mid=95';
        jQuery("#sendy-95").fadeOut('500');
        jQuery.ajax({
            url: 'https://shapebootstrap.net/modules/mod_sendy/ajax/subscribe.php',
            data: url,
            type: "POST",
            dataType: 'text json',
            success: function (ret)
            {
                if (ret.success == true)
                {
                    jQuery("#sendy-95").html(ret.html).fadeIn('1000');
                }
                else
                {
                    jQuery("#sendy-95").html("There was an error processing your subscription.").fadeIn('1000');
                }
            }
        });
        return false; // Don't actually submit the form
    });
</script></div></div></div></div></div></div></section><footer id="sp-footer"><div class="container"><div class="row"><div id="sp-footer1" class="col-sm-6 col-md-6"><div class="sp-column "><span class="sp-copyright"> Â© 2016 ShapeBootstrap. Some Rights Reserved. A <a target="_blank" href="http://www.joomshaper.com">JoomShaper</a> Family.</span></div></div><div id="sp-footer2" class="col-sm-6 col-md-6"><div class="sp-column "><div class="sp-module "><div class="sp-module-content">
