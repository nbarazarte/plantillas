<div class="comment-text">ok! Solved!<br/>
<br/>
<br/>
// Contact form<br/>
var form = $('#main-contact-form');<br/>
form.submit(function(event){<br/>
event.preventDefault();<br/>
var form_status = $('&lt;div class=&quot;form_status&quot;&gt;&lt;/div&gt;');<br/>
$.ajax({<br/>
type: &quot;POST&quot;,url: $(this).attr('action'),data: form.serialize(),<br/>
<br/>
beforeSend: function(){<br/>
form.prepend( form_status.html('&lt;p&gt;&lt;i class=&quot;fa fa-spinner fa-spin&quot;&gt;&lt;/i&gt; Enviando Email...&lt;/p&gt;').fadeIn() );<br/>
}<br/>
}).done(function(data){<br/>
form_status.html('&lt;p class=&quot;text-success&quot;&gt;' + data.message + '&lt;/p&gt;').delay(4000).fadeOut();<br/>
});<br/>
// reset form<br/>
form[0].reset();<br/>
});</div>
